<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.fefu.Exam.ExamMapper">

    <insert id="examIns" keyProperty="examId" useGeneratedKeys="true">
        INSERT INTO exam
        SET exam_id = #{examId}
        , semester_id = #{semesterId}
        , subject_id = #{subjectId}
        , option = #{option}
    </insert>

</mapper>